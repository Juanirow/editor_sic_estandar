EXA2	START 	0
ONE	EQU	1
TWO	EQU	2
THREE	EQU	3
SIX	EQU	TWO*THREE
FIVE	EQU	TWO+SIX/TWO
BUSCA	+LDB	#DIRMAX
	BASE	DIRMAX
	LDT	#MAX
	STT	DIRMAX
	LDCH	CAD
	STCH	@DIRMAX
	LDX	#(FIVE*TWO-SIX*THREE/TWO)
	LDS	#(ONE+SIX/THREE)
CICLO	LDCH	CAD,X
	+AND	#1000h
	RMO	A,T
	LDCH	@DIRMAX
	COMPR	T,A
	JGT	MAYOR
	J 	AVANZA
MAYOR	STCH	MAX
AVANZA	TIXR	S
	JLT	CICLO
	RSUB
CAD	BYTE	X'F3224'
	RESB	1000h
DIRMAX	RESW	1
MAX	RESB	1
MAIN	+JSUB	BUSCA
	CLEAR	A
	LDCH	MAX
	BYTE	X'FF'
	END	MAIN
